struct ll{next:^ll data:string};
pub fn main()->int{
   let a:^ll = new ll;
   (^a).data = "hello";
   for(let i:int = 0; i<10; i = i+1){
      let b:^ll = new ll;
      (^b).next = a;
      (^b).data = int_to_string(i);
      a = b;
   }
   let cur:^ll = a;
   while(cur){
      put_str((^cur).data+"\n");
      cur = (^cur).next;
   }
   a = false;
   return 0;
}

 
