struct test {a:int b:int c:int};

let test0:test = {0,0,0};

fn add(left:test right:test)->test{
    let out:test;
    out.a = left.a+right.a;
    out.b = left.b+right.b;
    out.c = left.c+right.c;
    return out;
}

fn main(args:[]string)->int{
   let a:test = {0,0,0};
   let b:test;
   a = a.add(b);
   for(let i:int = 0; i<10; i = i+1){
    a.b = a.b+1;
   }
}
 
