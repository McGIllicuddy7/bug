struct ll{
   next:^ll 
   data:string
};
fn get_next(this:^ll)->^ll{
   return this.next;
}
pub fn main()->int{
  let a = new ll;
  let prev = a;
   a.data = "howdy";
   a.next = new ll;
   let count = 24;
   for(let i =0; i<count; i = i+1){
      a.data = int_to_string(i);
      a.next = new ll;
      a = a.next;
   }
   for(let i = 0;i<count;i = i+1){
      put_str(prev.data);
      prev = prev.next;
      put_str("\n");
   } 
}

 
