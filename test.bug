struct vec2{
   x:float 
   y:float
};
pub fn +(a:vec2 b:vec2)->vec2{
   let out:vec2;
   out.x = a.x+b.x;
   out.y = a.y+b.y; 
   return out;
}
pub fn *(a:vec2 b:float)->vec2{
   let out:vec2;
   out.x = a.x*b;
   out.y = a.y*b;
   return out;
}
pub fn put_str_ln(a:vec2)->void;
pub fn main()->int{
   let a:vec2 = vec2{1.0,2.0};
   let b:vec2 = vec2{3.0, 4.0};
   let c:vec2;
   c = a*3.0+b;
   put_str_ln(c);
   return 0;
}

 
