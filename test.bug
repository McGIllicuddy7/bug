struct ll{
   next:^ll 
   data:string
};
fn get_next(this:^ll)->^ll{
   return this.next;
}
pub fn main()->int{
   let a = ll{false, "howdy"};
   let b:^ll = new ll;
   let c:^ll = new ll;
   let d:^ll = new ll;
   a.next = b;
   b.next = c;
   c.next = d;
   d.next = &a;
   a.get_next();
   put_str(a.data);
   put_str("\n");
   return 0;
}

 
