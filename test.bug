import toast.bug;
struct vec2{
   x:float
   y:float
};
pub fn *(a:vec2 b:float)->vec2{
   let out:vec2;
   out.x = a.x*b;
   out.y = a.y*b;
   return out;
}
pub fn +(a:vec2 b:vec2)->vec2{
   let out:vec2;
   out.x = a.x+b.x;
   out.y = a.y+b.y;
   return out;
}
pub fn put_str_ln(a:vec2)->void;
