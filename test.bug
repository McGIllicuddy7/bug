struct ll{
   next:^ll 
   data:string
};
fn get_next(this:^ll)->^ll{
   return this.next;
}
pub fn main()->int{
   let a = ll{false, "howdy"};
   let b = new ll;
   let c = new ll;
   let d = new ll;
   a.next = b;
   b.next = c;
   c.next = d;
   d.next = &a;
   a.get_next();
   if(a.next){
   	return 0;
   }
   else{
   	return 1;
   }
}

 
